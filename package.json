{
  "name": "react-glide",
  "version": "3.2.0",
  "description": "A multi purpose carousel module built in React.",
  "author": "andrewangelle",
  "repository": "http://github.com/andrewangelle/react-glide.git",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "dist/types/Glide.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js && npm run build:bundle",
    "build:types": "rm -rf dist && tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist/build --extensions \".tsx\" --ignore \"**/*.test.tsx\" --source-maps inline",
    "build:bundle": "rm -rf lib && webpack-cli --config ./webpack.config.js --mode production",
    "start": "webpack-dev-server --config ./example/webpack.config.js --mode development --open --hot",
    "test": "jest --watchAll",
    "test:once": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run build && git add -A lib"
  },
  "keywords": [
    "react",
    "component"
  ],
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x",
    "react-dom": "^0.14.6 || 15.x.x || 16.x.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/enzyme": "^3.1.18",
    "@types/jest": "^23.3.14",
    "@types/node": "^11.9.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-transition-group": "^2.0.15",
    "@types/webpack-env": "^1.13.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.0.0",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "postcss-modules-extract-imports": "^2.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {}
}
